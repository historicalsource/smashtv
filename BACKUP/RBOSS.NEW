	CALLA	SRAND
	ADD	A0,A7
	SRA	3,A10
	ADD	A10,A6
	SRA	3,A11
	ADD	A11,A7
	SRA	2,A6			;CUT DOWN VELOCITY
	SRA	2,A7
	MOVE	*A8(OXVAL),A0,L
	MOVE	*A8(OYVAL),A1,L
	CMPI	CLSENMY|TYPFACE|SUBER,A9
	JREQ	BLOE1
	ADDI	>F0000,A0		;ADJUST START COORD X ,Y
	ADDI	>E0000,A1	
	JRUC	BLOE2
BLOE1
	ADDI	>180000,A0
	ADDI	>E0000,A1
BLOE2
	MOVI	EYEFLY,A9
BLOE2A
	MOVI	I1,A2			;(OIMG)
	MOVI	231,A3			;(OZPOS)
	MOVI	DMAWNZ,A4		;(OFLAGS) NON-ZERO VALUE
	MOVI	CLSNEUT|TYPNEUT,A5	;(OID)
